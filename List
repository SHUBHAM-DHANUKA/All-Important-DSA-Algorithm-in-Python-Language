class node:
  def __init__(self):
    self.data = None
    self.next = None
  def setData(self,data):
    self.data = data
  def getData(self):
    return self.data
  def setNext(self, next):
    self.next = next
  def getNext(self):
    return self.next
  a = 5

class linkedList:
  start = node()
  first = node()
  second = node()
  
  start.setData(5)
  start.setNext(first)

  first.setData(5)
  first.setNext(second)
  
  second.setData(8)
  second.setNext('Null')

  print(start.next.a) 
  while ( start != 'Null'):
    print(start.getData())
    start = start.getNext()

 
