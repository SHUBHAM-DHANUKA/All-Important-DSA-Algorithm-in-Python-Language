def max1(a,b,c):
  if a>b and a>c:
    return a
  elif b>c:
    return b
  else:
    return c
print(max(5,8,9))

def sum1(li):
  sum2 = 0 
  for i in li:
    sum2 = sum2+i
  return sum2

print(sum1([1,2,3,4,5]))

def reverse(string):
  string1=''
  i = len(string)
  while(i>0):
    string1 = string1+ string[i-1]
    i = i-1
    
  return string1  
    
print(reverse("hi i am good"))
