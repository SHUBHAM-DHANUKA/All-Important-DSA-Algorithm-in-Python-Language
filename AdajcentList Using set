print("Graph Jouney Start!")
print("Adjaceny List")
class Graph:
  graph = {'A': set(['B', 'C']),
         'B': set(['A', 'D', 'E']),
         'C': set(['A', 'F']),
         'D': set(['B']),
         'E': set(['B', 'F']),
         'F': set(['C', 'E'])}
  
gh = Graph()
print(gh.graph['A'])
gh.graph['A'].add('k')
print(gh.graph['A'])
gh.graph['A'].remove('B')
print(gh.graph['A'])
