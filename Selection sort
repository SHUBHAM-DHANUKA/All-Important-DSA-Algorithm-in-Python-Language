print("sorting Journey")
print("Selection Sort")

#li = [64,25,12,22,11]
li = ['t','u','t','o','r','i','a','l']
print(li)
k = 0 
i = 0 
for j in range(len(li)):
  while(i<len(li)):
    if li[k] > li[i]:
      k = i
    i = i+1
  li[j], li[k] = li[k],li[j]
  #print(li[j],li[k])
  print(li)
  k = j+1
  i = k
print(li)




  


